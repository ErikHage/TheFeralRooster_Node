<!DOCTYPE html>
<html>
<head>
    <% include partials/headerCommon %>

    <title><%= title %></title>
</head>
<body>

    <!-- Header + Navbar -->
    <% include partials/pageHeader.ejs %>

    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-2"></div>
            <div class="col-sm-8">

                <!-- Fix styling on panels and links in titles -->

                <div class="panel-group panel-group-skills">
                <% skills.forEach(function(skill){ %>
                    <div class="panel panel-default panel-skills">
                        <div class="panel-heading panel-heading-skills">
                            <h3 class="panel-title-skills">
                               <a data-toggle="collapse"
                               href="<%= '#skills-' + skill.area %>">
                               <%= skill.area %></a></h3>
                        </div>
                        <div id="<%= 'skills-' + skill.area %>" class="panel-collapse collapse">
                            <div class="panel-body">
                                <ul>
                                <% for(var j=0; j<skill.skill.length; j++) { %>
                                    <li> <%= skill.skill[j] %> </li>
                                <% } %>
                                </ul>
                            </div>
                        </div>
                    </div>

                <% }); %>
                </div>

            </div>
            <div class="col-sm-2"></div>
        </div>
    </div>

</body>
<footer>
    <% include partials/footer %>
</footer>
</html>
